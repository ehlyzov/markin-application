<!DOCTYPE html>
<html>
<head>
  <title>Markin</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body class="lib-textFormatted">

<%= render 'lib/site_title' %>
<%= render 'lib/menu' %>

<% if content_for?(:main_content) %>
  <%= yield :main_content %>
<% else %>
  <%= yield %>
<% end %>


</body>
</html>
