<lib-source
  url="<%= api_content_holders_path(page_type: page.class.name, page_id: page.id, format: 'json') %>"
  receiver="pageContentBlocks">
</lib-source>

<lib-layout-cell width="<%= width %>">
  <lib-layout>
    <template id="pageContentBlocks" is="auto-binding" repeat="{{m in source}}">
      <fp-contentblock model="{{m}}"></fp-contentblock>
    </template>
  </lib-layout>
</lib-layout-cell>
